{
  "scopes": [
    {
      "name": "$XSAPPNAME.Write",
      "description": "Write_Role"
    },
    {
      "name": "$XSAPPNAME.Read",
      "description": "Read_Role"
    },
    {
      "name": "uaa.user",
      "description": "UAA"
    }
  ],
  "attributes": [],
  "role-templates": [
    {
      "name": "Write_Role",
      "description": "Role for writing",
      "scope-references": [
        "$XSAPPNAME.Write"
      ]
    },
    {
      "name": "Read_Role",
      "description": "Role for reading",
      "scope-references": [
        "$XSAPPNAME.Read"
      ]
    },
    {
      "name": "Token_Exchange",
      "description": "UAA",
      "scope-references": [
        "uaa.user"
      ]
    }
  ],
  "oauth2-configuration": {
    "credential-types": [
      "binding-secret",
      "x509"
    ],
    "redirect-uris": [
      "https://*.us10-001.hana.ondemand.com/**",
      "https://*.us10.hana.ondemand.com/**",
      "https://*.us10-001.hana.ondemand.com/login/callback"
    ]
  }
}
